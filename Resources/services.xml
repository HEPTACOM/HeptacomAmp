<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="heptacom_amp.subscriber.detail" class="HeptacomAmp\Subscriber\Detail">
            <tag name="shopware.event_subscriber" />
        </service>
        <service id="heptacom_amp.subscriber.AMP" class="HeptacomAmp\Subscriber\AMP">
            <tag name="shopware.event_subscriber" />
            <argument type="service" id="PluginLogger" index="0" />
        </service>
        <service id="heptacom_amp.subscriber.backend" class="HeptacomAmp\Subscriber\Backend">
            <tag name="shopware.event_subscriber" />
        </service>
        <service id="heptacom_amp.components.css_amplifier" class="HeptacomAmp\Components\CssAmplifier" />
    </services>
</container>
